function lookup(code: string): ISO3166Info | undefined {
    if (code.length > 3) {
        return undefined;
    }
    return map.get(code);
}

/**
 * Compare two ISO-3166-1 codes, even if they are of different formats.
 * 
 * No normalization whatsoever is performed on the supplied codes.
 * This does not pad numeric code values with zeroes, nor does it uppercase or
 * trim the codes. Though this would be a trivial accommodation, it is best to
 * be strict per [IETF RFC 9413](https://datatracker.ietf.org/doc/html/rfc9413).
 * This means that numeric codes with anything other than three digits, or
 * letter-based codes with lowercased letters, or any code with whitespace will
 * fail to be recognized.
 *
 * @param code1 Any ISO-3166-1 code (two-letter, three-letter, or numeric)
 * @param code2 Any ISO-3166-1 code (two-letter, three-letter, or numeric)
 * @returns `true` if the codes refer to the same country
 */
export
function compare(code1: string, code2: string): boolean {
    const c1 = lookup(code1);
    const c2 = lookup(code2);
    if (!c1 || !c2) {
        return false; // If either code is unrecognized, treat as a non-match.
    }
    return c1 === c2; // Compare objects by reference
}

/**
 * Check if an ISO-3166-1 code is valid.
 * 
 * No normalization whatsoever is performed on the supplied `code`.
 * This does not pad numeric code values with zeroes, nor does it uppercase or
 * trim the `code`. Though this would be a trivial accommodation, it is best to
 * be strict per [IETF RFC 9413](https://datatracker.ietf.org/doc/html/rfc9413).
 * This means that numeric codes with anything other than three digits, or
 * letter-based codes with lowercased letters, or any code with whitespace will
 * fail to be recognized.
 * 
 * @param code Any ISO-3166-1 code (two-letter, three-letter, or numeric)
 * @returns `true` if the `code` is valid
 */
export
function isValid(code: string): boolean {
    if (code.length > 3) {
        return false;
    }
    return !!map.get(code);
}
